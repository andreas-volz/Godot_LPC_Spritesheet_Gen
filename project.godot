config_version=5

[application]

config/name="LPC Gen Demo"
run/main_scene="res://demo/lpc_sprite_demo.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[editor_plugins]

enabled=PackedStringArray("res://addons/lpc_spritesheet_gen/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
